{
  "name": "alloc_gpus_busy",
  "command": "./alloc_gpus_busy.drain.sh",
  "platforms": ["8xGPU", "1xGPU"],
  "skip_for_cpu_jobs": true,
  "skip_for_partial_gpu_jobs": false,
  "skip_for_reservation_prefixes": [],
